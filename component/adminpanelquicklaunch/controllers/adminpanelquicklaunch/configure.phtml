<script type="text/javascript">    jQuery(document).ready(function () {        jQuery(".incon_reference_link,.incon_reference_link_remove").click(function (event) {            var myid = jQuery(this).attr('myid');            var id = jQuery(this).attr('class');            var referedobj = jQuery('#incon_reference_' + myid);            var title = referedobj.val();            if(id.search("remove") > 0){                title = '';            }            var mylink = referedobj.attr('mylink');            var iconpath = referedobj.attr('iconpath');            var fetchtype = referedobj.attr('fetchtype');            jQuery(this).find('img').attr('src',siteInfo.baseUrl + '/images/loading.gif');            jQuery.ajax({                type: "POST",                url: siteInfo.baseUrl + '/adminpanelquicklaunch/configure',                data: {title: title, iconpath:iconpath,mylink: mylink,fetchtype: fetchtype}            })                    .done(function (responseTxt) {                        var responseObj = eval('(' + responseTxt + ')');                        location.reload()                    });        });    });</script>  <?php 	App::Helper('Toolbar')->setTitle("List")->setBtnBack()->Render(array('code' => 'accounts_top')); ?>	<?php$DashBoardNAVs = App::Module('ACL')->getDashboardNAVS();$DataGrid = App::Module('DataGrid')        ->clear()        ->setHeader(array('ICON','GROUP', 'NAME', 'LOCATION', 'RENAME', 'OPTIONS'))        ->setFooter();$adminpanelquickList = App::Component('adminpanelquicklaunch')->Helper('Data')->listOfLinks();$i = 1;foreach ($DashBoardNAVs as $node) {    foreach ($node['child'] as $childnode) {        $fetchType = 'URL';        $icon_path = '/themeroot/admin/images/icons/info.jpg';        if (isset($childnode['adminicon'])) {            if (!empty($childnode['adminicon'])) {                $fetchType = $childnode['adminicon']['type'];                $icon_path = $childnode['adminicon']['location'];            } else {                $icon_path = '/themeroot/admin/images/icons/' . App::Helper('utility')->text2Normalize($childnode['title']) . '.jpg';            }            if (!file_exists(App::Config()->basedir("$icon_path")) && !file_exists(App::Config()->rootDir("$icon_path"))) {                $fetchType = 'URL';                $icon_path = '/themeroot/admin/images/icons/info.jpg';            }        }        if (strtolower($fetchType) == 'url') {            $icon = App::load("Helper/Html")->imgTag(App::Config()->baseurl($icon_path), null, array("height" => "20", "width" => "20"));        } else {            $icon = App::load("Helper/Html")->imgDTag(App::Config()->rootDir($icon_path), '/20/fix', array("height" => "20", "width" => "20"));        }                foreach ($childnode['items'] as $row) {            $btn = App::Html()->linkTag(null, App::Html()->imgTag(App::Config()->baseUrl("/images/admin/add.gif")), array('myid' => "{$i}", 'class' => 'incon_reference_link'));                        $name2save = $row['title'];            if(isset($adminpanelquickList[$row['link']])){                $name2save = $adminpanelquickList[$row['link']]->title;                $btn = App::Html()->linkTag(null, App::Html()->imgTag(App::Config()->baseUrl("/images/admin/edit.gif")), array('myid' => "{$i}", 'class' => 'incon_reference_link'));                $btn .= ' ' . App::Html()->linkTag(null, App::Html()->imgTag(App::Config()->baseUrl("/images/admin/remove.gif")), array('myid' => "{$i}", 'class' => 'confirm-link incon_reference_link_remove'));              }            $DataGrid->addRow(                    $icon, "{$node['parent']['title']}>{$childnode['title']}","{$row['title']}", $row['link'], App::Html()->inputTag('', substr($name2save, 0, 12), array('id' => 'incon_reference_' . $i, 'fetchtype' => $fetchType,'iconpath' => $icon_path,'mylink' => $row['link'], 'class' => ' large', 'maxlength' => '12')),                                        App::Html()->linkTag(null, $btn, array('myid' => "{$i}", 'class' => 'incon_reference_link'))            );            $i++;        }    }}$DataGrid->Render();?>  <?phpApp::Helper('Toolbar')->setBtnBack()->Render(array('code' => 'accounts_bottom'));